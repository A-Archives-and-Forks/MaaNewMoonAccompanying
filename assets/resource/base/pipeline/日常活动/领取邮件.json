{
    "__yamaape_config_领取邮件": {
        "filename": "领取邮件",
        "version": "v0.3.4",
        "export": "",
        "prefix": "领取邮件"
    },
    "领取邮件_领取邮件": {
        "recognition": "TemplateMatch",
        "action": "Click",
        "template": [
            "../image/email.png"
        ],
        "pre_delay": 400,
        "post_delay": 1000,
        "focus": "> 进入邮件面板",
        "timeout": 2000,
        "__yamaape": {
            "position": {
                "x": 351,
                "y": -70
            }
        },
        "next": [
            "领取邮件_领取全部邮件"
        ],
        "on_error": [
            "领取邮件_领取结束"
        ]
    },
    "领取邮件_领取全部邮件": {
        "recognition": "OCR",
        "action": "Click",
        "expected": [
            "领取全部"
        ],
        "pre_delay": 800,
        "timeout": 2000,
        "__yamaape": {
            "position": {
                "x": 611,
                "y": -70
            }
        },
        "next": [
            "领取邮件_获取邮件奖励",
            "领取邮件_暂无奖励"
        ],
        "on_error": [
            "领取邮件_领取结束"
        ]
    },
    "领取邮件_领取结束": {
        "recognition": "DirectHit",
        "action": "DoNothing",
        "timeout": 2000,
        "__yamaape": {
            "position": {
                "x": 1172,
                "y": 5
            }
        },
        "next": [
            "领取邮件_返回主页面"
        ]
    },
    "领取邮件_获取邮件奖励": {
        "recognition": "OCR",
        "action": "Click",
        "expected": [
            "点击以继续"
        ],
        "pre_delay": 800,
        "focus": "> 领取全部邮件",
        "__yamaape": {
            "position": {
                "x": 889,
                "y": -150
            }
        },
        "next": [
            "领取邮件_领取结束"
        ]
    },
    "领取邮件_暂无奖励": {
        "recognition": "OCR",
        "action": "Click",
        "expected": [
            "当前暂无奖励可领取"
        ],
        "pre_delay": 1,
        "post_delay": 1,
        "focus": "> 暂无邮件可领取",
        "__yamaape": {
            "position": {
                "x": 888,
                "y": 97
            }
        },
        "next": [
            "领取邮件_领取结束"
        ]
    },
    "领取邮件_返回主页面": {
        "recognition": "DirectHit",
        "action": "Click",
        "target": [
            15,
            60,
            1,
            1
        ],
        "post_delay": 5000,
        "focus": "> 返回主界面",
        "__yamaape": {
            "position": {
                "x": 1460,
                "y": -58
            }
        }
    },
    "领取邮件_删除已读": {
        "recognition": "OCR",
        "action": "Click",
        "expected": [
            "删除已读"
        ],
        "focus": "> 删除已读邮件",
        "__yamaape": {
            "position": {
                "x": 1193,
                "y": 174
            }
        },
        "next": [
            "领取邮件_返回主页面"
        ]
    },
    "领取邮件_开始": {
        "recognition": "DirectHit",
        "action": "DoNothing",
        "__yamaape": {
            "position": {
                "x": 80,
                "y": -59
            }
        },
        "next": [
            "领取邮件_领取邮件"
        ]
    }
}
