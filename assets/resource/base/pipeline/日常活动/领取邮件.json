            "position": {
                "x": 1193,
                "y": 174
            }
        },
        "next": [
            "领取邮件_返回主页面"
        ]
    },
    "领取邮件_开始": {
        "recognition": "DirectHit",
        "action": "DoNothing",
        "__yamaape": {
            "position": {
                "x": -200,
